<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <table-filter match-name="BIN$.*" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="CHANGELOG" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="JBPM.*" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="PARTICIPANTIMPL" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="TASKIMPL" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="TASKDEFINITIONIMPL" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="SWIMLANEIMPL" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="SWIMLANEDEFINITIONIMPL" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="ACL.*" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="DEPT" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="MENU" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="RESC" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="ROLE" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="USER" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="MENU_ROLE" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="RESC_ROLE" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="USER_ROLE" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name=".*" match-schema="PUBLIC" />

  <table schema="PUBLIC" name="DEPARTMENT" class="com.family168.leona.domain.Department">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="STATE" class="com.family168.leona.domain.State">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="JOB" class="com.family168.leona.domain.Job">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="AFFICE" class="com.family168.leona.domain.Affice">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="BUMF" class="com.family168.leona.domain.Bumf">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
    <foreign-key constraint-name="FK_BUMF_SENDER">
      <many-to-one property="sender"/>
      <set property="senderBumfs"/>
    </foreign-key>
    <foreign-key constraint-name="FK_BUMF_ACCEPTER">
      <many-to-one property="accepter"/>
      <set property="accepterBumfs"/>
    </foreign-key>
  </table>
  <table schema="PUBLIC" name="EMPLOYEE" class="com.family168.leona.domain.Employee">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="MESSAGE" class="com.family168.leona.domain.Message">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
    <foreign-key constraint-name="FK_MESSAGE_SENDER">
      <many-to-one property="sender"/>
      <set property="senderMessages"/>
    </foreign-key>
    <foreign-key constraint-name="FK_MESSAGE_ACCEPTER">
      <many-to-one property="accepter"/>
      <set property="accepterMessages"/>
    </foreign-key>
  </table>
  <table schema="PUBLIC" name="SIGN" class="com.family168.leona.domain.Sign">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>
  <table schema="PUBLIC" name="SIGN_STATE" class="com.family168.leona.domain.SignState">
    <primary-key>
      <generator class="native" />
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>

