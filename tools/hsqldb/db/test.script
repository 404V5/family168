CREATE MEMORY TABLE AFFICE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,TITLE VARCHAR(40),TIME TIMESTAMP,EMPLOYEE_ID BIGINT,CONTENT VARCHAR(2000),CONSTRAINT PK_AFFICE PRIMARY KEY(ID),CONSTRAINT FK_AFFICE_EMPLOYEE FOREIGN KEY(EMPLOYEE_ID) REFERENCES EMPLOYEE(ID))
CREATE MEMORY TABLE BUMF(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,SENDER BIGINT,ACCEPTER BIGINT,TITLE VARCHAR(40),TIME TIMESTAMP,CONTENT VARCHAR(2000),AFFIX VARCHAR(40),EXAMINE INTEGER,SIGN INTEGER,CONSTRAINT PK_BUMF PRIMARY KEY(ID),CONSTRAINT FK_BUMF_SENDER FOREIGN KEY(SENDER) REFERENCES EMPLOYEE(ID),CONSTRAINT FK_BUMF_ACCEPTER FOREIGN KEY(ACCEPTER) REFERENCES EMPLOYEE(ID))
CREATE MEMORY TABLE MESSAGE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,TITLE VARCHAR(40),TIME TIMESTAMP,SENDER BIGINT,ACCEPTER BIGINT,CONTENT VARCHAR(2000),STATUS INTEGER,CONSTRAINT PK_MESSAGE PRIMARY KEY(ID),CONSTRAINT FK_MESSAGE_SENDER FOREIGN KEY(SENDER) REFERENCES EMPLOYEE(ID),CONSTRAINT FK_MESSAGE_ACCEPTER FOREIGN KEY(ACCEPTER) REFERENCES EMPLOYEE(ID))
CREATE MEMORY TABLE SIGN_STATE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,NAME VARCHAR(20),TIME TIMESTAMP,CONSTRAINT PK_SIGN_STATE PRIMARY KEY(ID))
CREATE MEMORY TABLE SIGN(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,TIME TIMESTAMP,EMPLOYEE_ID BIGINT,LATE INTEGER,QUIT INTEGER,LEAVE INTEGER,WORK INTEGER,SIGN_STATE_ID BIGINT,CONSTRAINT PK_SIGN PRIMARY KEY(ID),CONSTRAINT FK_SIGN_EMPLOYEE FOREIGN KEY(EMPLOYEE_ID) REFERENCES EMPLOYEE(ID),CONSTRAINT FK_SIGN_SIGN_STATE FOREIGN KEY(SIGN_STATE_ID) REFERENCES SIGN_STATE(ID))
